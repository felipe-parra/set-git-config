{
  "name": "set-git-config",
  "version": "1.0.9",
  "description": "A simple CLI to set git config",
  "main": "./dist/index.js",
  "bin": {
    "init": "./dist/index.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "init": "node ./dist/index.js",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "start": "node ./dist/index.js",
    "dev": "npm-run-all build start"
  },
  "keywords": [
    "git",
    "config",
    "cli"
  ],
  "author": {
    "name": "Felipe Parra",

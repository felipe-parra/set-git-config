  },
  "license": "GNU General Public License v3.0",
  "dependencies": {
    "inquirer": "^9.1.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.3.0"
  },
  "type": "module",
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}